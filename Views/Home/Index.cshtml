@{
    ViewData["Title"] = "Home";
}

<!-- 🔹 Hero Section -->
<div class="p-5 text-center bg-dark text-light rounded-3 shadow">
    <h1 class="display-4 fw-bold text-danger">Welcome to Cinema 🎬</h1>
    <p class="lead">Discover the latest movies, book your tickets, and enjoy the magic of cinema!</p>
    <a asp-controller="Movies" asp-action="Index" class="btn btn-danger btn-lg mt-3">🎟️ Browse Movies</a>
</div>

<!-- 🔹 Featured Movies -->
<div class="container mt-5">
    <h2 class="text-light mb-4">🔥 Featured Movies</h2>
    <div class="row g-4">
        <!-- مثال لفيلم -->
        <div class="col-md-4">
            <div class="card bg-black text-light shadow h-100">
                <img src="data:image/jpeg;base64," class="card-img-top" alt="Movie Poster">
                <div class="card-body">
                    <h5 class="card-title">Movie Title</h5>
                    <p class="card-text">Short description about the movie goes here...</p>
                    <a asp-controller="Movies" asp-action="Details" asp-route-id="1" class="btn btn-outline-danger">View Details</a>
                </div>
            </div>
        </div>

        <div class="col-md-4">
            <div class="card bg-black text-light shadow h-100">
                <img src="data:image/jpeg;base64," class="card-img-top" alt="Movie Poster">
                <div class="card-body">
                    <h5 class="card-title">Another Movie</h5>
                    <p class="card-text">Short description about the movie goes here...</p>
                    <a asp-controller="Movies" asp-action="Details" asp-route-id="2" class="btn btn-outline-danger">View Details</a>
                </div>
            </div>
        </div>

        <div class="col-md-4">
            <div class="card bg-black text-light shadow h-100">
                <img src="data:image/jpeg;base64," class="card-img-top" alt="Movie Poster">
                <div class="card-body">
                    <h5 class="card-title">Third Movie</h5>
                    <p class="card-text">Short description about the movie goes here...</p>
                    <a asp-controller="Movies" asp-action="Details" asp-route-id="3" class="btn btn-outline-danger">View Details</a>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- 🔹 Call to Action -->
<div class="text-center mt-5">
    <h3 class="fw-bold">🍿 Ready for the Show?</h3>
    <p>Book your ticket now and enjoy the big screen experience!</p>
    <a asp-controller="Cart" asp-action="Index" class="btn btn-danger btn-lg">Go to Cart 🛒</a>
</div>
